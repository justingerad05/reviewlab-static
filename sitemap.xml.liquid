---
permalink: sitemap.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for post in posts %}
  <url>
    <loc>{{ post.url }}</loc>
    <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
  </url>
{% endfor %}
</urlset>
